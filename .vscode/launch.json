    "version": "0.2.0",
    "configurations": [
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/aarch64/bin/imagenet-camera",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/build/aarch64",
            "environment": [],
            "externalConsole": true,
            "MIMode": "aarch64-linux-gnu-gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}